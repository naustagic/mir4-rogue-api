// prisma/schema.prisma
generator client {
  provider = "prisma-client-js"
  output   = "./generated/client"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Code {
  id           Int     @id @default(autoincrement())
  price        Int
  cardCode     String
  cardPassword String
  emailOwner   String?
}
